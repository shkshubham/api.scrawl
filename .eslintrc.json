{
    "parser": "babel-eslint",
    "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module",
        "ecmaFeatures": {
            "experimentalObjectRestSpread": true
        }
    },
    "extends": ["eslint:recommended", "google"],
    "rules": {
        // Additional, per-project rules...
        "require-jsdoc": "off",
        "no-undef": "off",
        "new-cap": "off"
    }
}